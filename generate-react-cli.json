{
  "usesTypeScript": true,
  "usesCssModule": true,
  "cssPreprocessor": "css",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "customTemplates": {
        "component": "templates/component/TemplateName.tsx"
      },
      "path": "components",
      "withStyle": false,
      "withTest": false,
      "withStory": false,
      "withLazy": false
    },
    "page": {
      "customTemplates": {
        "component": "templates/page/index.tsx"
      },
      "path": "pages",
      "withLazy": false,
      "withStory": false,
      "withStyle": false,
      "withTest": false
    },
    "layout": {
      "customTemplates": {
        "component": "templates/layout/TemplateName.tsx"
      },
      "path": "layouts",
      "withLazy": false,
      "withStory": false,
      "withStyle": false,
      "withTest": false
    }
  }
}
